<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GratitudeApp" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Primary brand color. -->

        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorAccent">#4caf50</item>
        <item name="colorSecondary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondaryVariant">@color/purple_500</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="textColorAll">#eceff1</item>
        <item name="buttonColorAll">#4caf50</item>
        <item name="askIconColor">#4caf50</item>
        <item name="HintTextColor">#B5ECEFF1</item>
        <item name="BackgroundColorAll">#263238</item>
        <item name="BackgroundLightColor">#62727b</item>
        <item name="CalendarTextColor">@color/white</item>


        <item name="android:statusBarColor" tools:targetApi="l">?attr/BackgroundColorAll</item>
        <!-- Customize your theme here. -->
    </style>



